{
  "name": "yandex-cloud-fn",
  "description": "Helpers for Yandex Cloud function Node.js runtime",
  "version": "1.0.0",
  "main": "dist/index.js",
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint src test --ext .ts",
    "test": "mocha -r ts-node/register test/setup.ts test/specs/*.ts",
    "test:l": "LOG_LEVEL=info npm t",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "release": "npm run lint && npm t && npm run build && npm version $VER && npm publish && git push --follow-tags --no-verify",
    "release-patch": "VER=patch npm run release",
    "release-minor": "VER=minor npm run release"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": "eslint --ext .ts"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "alice-types": "^1.3.0"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-plugin-mocha": "^9.0.0",
    "lint-staged": "^11.2.3",
    "mocha": "^9.1.3",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "author": {
    "name": "Vitaliy Potapov"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/yandex-cloud-fn.git"
  },
  "license": "MIT"
}
